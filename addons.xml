    <language></language>
    <platform>all</platform>
    <license>The MIT License</license>
    <forum>http://forum.xbmc.org/showthread.php?tid=129499</forum>
    <source>https://github.com/robweber/xbmcbackup</source>
  </extension>
</addon>







